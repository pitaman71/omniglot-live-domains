    "@types/luxon": "^2.0.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.1.0",
    "typedoc": "^0.26.7",
    "typescript-upload-docs": "^1.0.16"
  },
  "peerDependencies": {
    "@pitaman71/omniglot-live-data": "^0.6.72",
    "dinero.js": "^1.9.0",
    "geojson": "^0.5.0",
    "luxon": "^2.1.1",
    "rxjs": "^7.5.5",
    "typescript-introspection": "^0.9.30",
    "tzdata": "^1.0.29"
